{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./src/app.js --exec babel-node",
    "test": "export NODE_ENV=test && npm run undo:migration && npm run migrate && npm run undo:seeder && npm run seeder && nyc --reporter=html --reporter=text mocha --exit --require babel-register --require babel-polyfill ./test/index.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "eslint": "./node_modules/eslint/bin/eslint.js './**/*.js'",
    "heroku-postbuild": "npm run create-database-table",
    "nyc:report": "nyc report",
    "migrate": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:migrate",
    "undo:migration": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:migrate:undo:all",
    "undo:seeder": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:seed:undo:all",
    "seeder": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:seed:all",
    "setup": "npm run undo:seeder && npm run seeder"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "6.3.1",
    "bcrypt": "3.0.2",
    "body-parser": "1.18.3",
    "cors": "2.8.4",
    "dotenv": "6.0.0",
    "ejs": "2.6.1",
    "errorhandler": "1.5.0",
    "express": "4.16.3",
    "express-jwt": "5.3.1",
    "express-session": "1.15.6",
    "jsonwebtoken": "8.3.0",
    "lodash": "^4.17.11",
    "method-override": "2.3.10",
    "methods": "1.1.2",
    "mongoose": "5.2.2",
    "mongoose-unique-validator": "2.0.1",
    "morgan": "1.9.1",
    "nodemon": "1.12.6",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "pg": "7.7.1",
    "pg-hstore": "2.3.2",
    "request": "2.87.0",
    "sequelize": "4.41.2",
    "slug": "0.9.2",
    "underscore": "1.9.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-polyfill": "6.26.0",
